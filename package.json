  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=3000 --config config/webpack.dev.js --profile",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "prebuild": "npm run clean:build",
    "postbuild": "gulp package",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --config config/webpack.dev.js --progress --color --profile",
    "build:prod": "webpack --config config/webpack.prod.js --progress --color --profile",
    "clean:build": "npm cache clean && npm run rimraf -- \"build/**/*\" ",
    "rimraf": "rimraf",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "aotconfig": "node_modules/.bin/ngc -p tsconfig.aot.json"
  },
  "private": true,
